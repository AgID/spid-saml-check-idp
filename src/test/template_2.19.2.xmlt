<samlp:AuthnRequest 
    xmlns:samlp="urn:oasis:names:tc:SAML:2.0:protocol"
    xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion" 
    ID="{{RequestID}}" 
    Version="2.0" 
    IssueInstant="{{IssueInstant}}" 
    Destination="{{Destination}}" 
    ForceAuthn="{{ForceAuthn}}" 
    AssertionConsumerServiceURL="{{AssertionConsumerServiceURL}}" 
    ProtocolBinding="{{ProtocolBinding}}" 
    AttributeConsumingServiceIndex="{{AttributeConsumingServiceIndex}}">

    <saml:Issuer 
        NameQualifier="{{IssuerNameQualifier}}" 
        Format="{{IssuerFormat}}">{{Issuer}}</saml:Issuer>

    <samlp:Extensions>
        <spid:RequiredAttributeAuthority xmlns:spid="https://spid.gov.it/saml-extensions">
            <Location></Location>
            <Location></Location>
            <Location></Location>
        </spid:RequiredAttributeAuthority>
    </samlp:Extensions>
            
    <samlp:NameIDPolicy Format="{{NameIDPolicyFormat}}" />
    <samlp:RequestedAuthnContext 
        Comparison="{{Comparison}}">
        <saml:AuthnContextClassRef>{{AuthnContextClassRef}}</saml:AuthnContextClassRef>
    </samlp:RequestedAuthnContext>

</samlp:AuthnRequest>